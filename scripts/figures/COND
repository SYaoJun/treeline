run_command(
  name="ycsb-main",
  run="python3 plot_ycsb.py",
  deps=[
    "//scripts/ycsb_v2:combine-main-zipfian",
  ],
)

run_command(
  name="rw_sweep",
  run="python3 plot_rw_sweep.py",
  options={
    "distribution": "zipfian",
    "record_size_bytes": 64,
    "y_min": 0,
    "y_max": 500,
  },
  deps=["//scripts/rw_sweep:combine"],
)

run_command(
  name="factor",
  run="python3 format_factor.py",
  deps=[
    "//scripts/ycsb_v2:combine-factor",
  ]
)
